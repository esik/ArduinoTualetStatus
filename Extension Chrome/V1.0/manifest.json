{
    "name"             : "WC",
    "description"      : "WC",
    "manifest_version" : 2,
    "version"          : "0.0.1",
    "icons"            : {
        "16"  : "icons/icon16.png",
        "32"  : "icons/icon32.png",
        "48"  : "icons/icon48.png",
        "100" : "icons/icon100.png",
        "128" : "icons/icon128.png"
    },
    "browser_action"   : {
        "default_icon"  : {
            "16"  : "icons/icon16.png",
            "32"  : "icons/icon32.png",
            "48"  : "icons/icon48.png",
            "100" : "icons/icon100.png",
            "128" : "icons/icon128.png"
        },
        "default_title" : "WC"
    },
    "permissions"      : [
        "*://192.168.0.171/*",
        "alarms"
    ],
    "background"       : {
        "persistent" : false,
        "scripts"    : [
            "scripts/ajax.js",
            "scripts/background.js"
        ]
    }
}